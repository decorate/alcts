{
  "name": "@team-decorate/alcts",
  "version": "2.1.9",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build:esm": "tsc -p tsconfig.esm.json && node scripts/fix-esm-imports.cjs",
    "build:cjs": "tsc -p tsconfig.cjs.json && node scripts/fix-cjs-imports.cjs && find dist-cjs -name '*.js' -exec sh -c 'mv \"$1\" \"${1%.js}.cjs\"' _ {} \\; && cp -r dist-cjs/* dist/ && rm -rf dist-cjs",
    "build": "rm -rf dist dist-cjs && npm run build:esm && npm run build:cjs",
    "test": "jest --forceExit -i",
    "type-check": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/decorate/alcts.git"
  },
  "keywords": [
    "ts",
    "model",
    "mapper"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/pluralize": "^0.0.29",
    "pluralize": "^8.0.0"
  },
  "devDependencies": {
    "@team-decorate/alcts": "^1.1.1",
    "@types/jest": "^29.2.1",
    "core-js": "^3.6.5",
    "jest": "^29.2.2",
    "jest-environment-jsdom": "^29.2.2",
    "jest-node-exports-resolver": "^1.1.6",
    "linq": "^3.2.3",
    "ts-jest": "^29.0.3",
    "tslib": "^2.8.1",
    "typescript": "^4.8.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "json"
    ],
    "transform": {
      ".*\\.(ts|js)x?$": "ts-jest"
    },
    "testRegex": ".*\\.(test|spec)\\.ts$",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transformIgnorePatterns": [],
    "testEnvironment": "node",
    "globals": {
      "window": {}
    }
  }
}
